/*!
 * FTModel fasttext 模型对象
 *
 */
interface FTModel : object
{
    /*! @brief 预测文本的分类
     @param text 文本
     @param k 返回前 k 个分类
     @return 返回分类结果
     */
    NArray predict(String text, Integer k = 3) async;

    /*! @brief 预测文本的分类
     @param texts 文本数组
     @param k 返回前 k 个分类
     @return 返回分类结果
     */
    NArray predict(Array texts, Integer k = 3) async;
};
